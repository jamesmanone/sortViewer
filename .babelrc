{
  "presets": ["es2015", "react", "stage-1"],
  "plugins": [
    "transform-class-properties",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ],
  "env": {
    "development": {
      "presets": ["stage-1"],
      "plugins": [
        "transform-class-properties",
        ["transform-runtime", {
          "polyfill": false,
          "regenerator": true
        }]
      ]
    }
  }
}
